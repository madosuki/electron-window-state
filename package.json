{
  "name": "@madosuki/electron-window-state",
  "version": "5.1.0",
  "description": "Simple module that helps to save and restore size and position of Electron windows.",
  "license": "MIT",
  "repository": "madosuki/electron-window-state",
  "main": "dist/index.js",
  "author": "Marcel Wiehle",
  "contributors": [
    "madosuki"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "xo && ava",
    "build": "tsc --build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "electron"
  ],
  "dependencies": {
    "jsonfile": "^6.1.0",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "electron": "^17.1.0",
    "@types/jsonfile": "^6.1.0",
    "@types/mkdirp": "^1.0.2",
    "ava": "^4.0.1",
    "mockery": "^2.1.0",
    "sinon": "^13.0.1",
    "typescript": "^4.5.5",
    "xo": "^0.48.0"
  },
  "xo": {
    "space": true,
    "ignores": [
      "index.d.ts"
    ],
    "rules": {
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "prefer-object-spread": 0
    }
  }
}
